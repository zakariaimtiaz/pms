	<parameter name="goalId" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT @row_number:=CASE WHEN @actionid=tmp.action_id THEN @row_number+1 ELSE 1 END AS row_number,@actionid:=tmp.action_id AS action_id,t_mon_var,
sequence,actions,start,end,indicator,indicator_type,ind_remarks,remarks,res_person,project,support_depart,
CASE WHEN POSITION("%" IN indicator_type)!=0 THEN CONCAT(mon_tar, '%') ELSE mon_tar END AS mon_tar,
CASE WHEN POSITION("%" IN indicator_type)!=0 THEN CONCAT(mon_acv, '%') ELSE mon_acv END AS mon_acv,
CONCAT(FLOOR((mon_acv/mon_tar * 100)-100), '%') AS mon_var,
CASE WHEN POSITION("%" IN indicator_type)!=0 THEN CONCAT(cum_tar, '%') ELSE cum_tar END AS cum_tar,
CASE WHEN POSITION("%" IN indicator_type)!=0 THEN CONCAT(cum_acv, '%') ELSE cum_acv END AS cum_acv,
CONCAT(FLOOR((cum_acv/cum_tar * 100)-100), '%') AS cum_var,
CASE WHEN POSITION("%" IN indicator_type)!=0 THEN CONCAT(tot_tar, '%') ELSE tot_tar END AS tot_tar
FROM (SELECT a.id AS action_id,@sequence:=a.sequence AS sequence,
a.actions,a.start,a.end, ai.indicator,ai.indicator_type,idd.remarks ind_remarks,
SUM(CASE WHEN  cm.sl_index=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.target,0) ELSE 0 END) t_mon_var,
SUM(CASE WHEN  cm.sl_index=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.target,0) ELSE 0 END) mon_tar,
SUM(CASE WHEN  cm.sl_index=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.achievement,0) ELSE 0 END) mon_acv,
CASE WHEN  ai.indicator_type LIKE 'Repeatable%' THEN
                 FLOOR(SUM(CASE WHEN  cm.sl_index<=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.target,0) ELSE 0 END)/SUM(CASE WHEN  cm.sl_index<=MONTH(DATE($P{currentMonth})) THEN 1 ELSE 0 END))
                 ELSE
SUM(CASE WHEN  cm.sl_index<=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.target,0) ELSE 0 END)  END cum_tar,
CASE WHEN  ai.indicator_type LIKE 'Repeatable%' THEN
                 FLOOR(SUM(CASE WHEN  cm.sl_index<=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.achievement,0) ELSE 0 END)/SUM(CASE WHEN  cm.sl_index<=MONTH(DATE($P{currentMonth})) THEN 1 ELSE 0 END))
                 ELSE
                 SUM(CASE WHEN  cm.sl_index<=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.achievement,0) ELSE 0 END)  END cum_acv,
CASE WHEN  ai.indicator_type LIKE 'Repeatable%' THEN
                 SUM(CASE WHEN  cm.sl_index=MONTH(DATE($P{currentMonth})) THEN COALESCE(idd.target,0) ELSE 0 END)
                 ELSE SUM(COALESCE(idd.target,0)) END  tot_tar,

                 a.note remarks,SUBSTRING_INDEX(a.res_person,'(',1) AS res_person,
                 (SELECT GROUP_CONCAT(short_name SEPARATOR ', ') FROM pm_projects WHERE LOCATE(CONCAT(',',id,',') ,CONCAT(',',a.source_of_fund,', '))>0 ) project,
                 (SELECT GROUP_CONCAT(short_name SEPARATOR ', ') FROM pm_service_sector WHERE LOCATE(CONCAT(',',id,',') ,CONCAT(',',a.support_department,','))>0 ) support_depart

                FROM (SELECT * FROM pm_actions  WHERE  YEAR(DATE(`start`))=$P{year} AND goal_id = $P{goalId}) a
                JOIN pm_goals g ON g.id = $P{goalId}
                JOIN pm_actions_indicator ai ON ai.actions_id = a.id
                JOIN pm_actions_indicator_details idd ON idd.indicator_id = ai.id
                JOIN custom_month cm ON cm.name=idd.month_name
                JOIN (SELECT * FROM pm_service_sector WHERE id = $P{serviceId}) sc ON sc.id = a.service_id
                GROUP BY ai.id
                ORDER BY sc.id,EXTRACT(YEAR FROM a.start) , a.goal_id ,a.tmp_seq) tmp,
(SELECT @rownum := 0,@actionid:=0) r
 WHERE tmp.t_mon_var!=0;]]>
	</queryString>
